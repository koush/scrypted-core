{
   "name": "@scrypted/core",
   "version": "0.0.1",
   "description": "Scrypted Core platform. websocket+engine.io api and web interface",
   "author": "Scrypted",
   "license": "Apache-2.0",
   "scripts": {
      "prebuild": "cd ../web && npm run build",
      "build": "NODE_ENV=production scrypted-webpack",
      "prepack": "npm run build",
      "asset": "rm -rf *.tgz && npm pack && mkdir -p dist/scrypted-core-pack && rm -rf dist/scrypted-core-pack/* && mv scrypted-core-*.tgz dist/scrypted-core-pack/scrypted-core.tgz",

      "prescrypted-vscode-launch": "scrypted-webpack",
      "scrypted-vscode-launch": "scrypted-deploy-debug",
      "scrypted-deploy-debug": "scrypted-deploy-debug",
      "scrypted-debug": "scrypted-debug",
      "scrypted-deploy": "scrypted-deploy",
      "scrypted-webpack": "scrypted-webpack"
   },
   "scrypted": {
      "name": "Scrypted Core",
      "type": "Device",
      "interfaces": [
         "HttpRequestHandler",
         "EngineIOHandler"
      ]
   },
   "dependencies": {
      "@scrypted/sdk": "0.0.37",
      "axios": "^0.19.0",
      "router": "^1.3.3",
      "url-parse": "^1.4.7"
   },
   "devDependencies": {}
}
